@model IEnumerable<Nunana.ViewModels.RentalListViewModel>

@{
    ViewBag.Title = "CANCELLED RENTALS";
}

<h3 class="text-info">CANCELLED RENTALS</h3> <br />

<table class="table table-striped table-responsive dataTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RoomNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TenantName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateCancelled)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CancelledBy)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.RoomNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TenantName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DateCancelled)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CancelledBy)
                </td>
            </tr>
        }
    </tbody>
</table>

